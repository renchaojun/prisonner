<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
 <mapper namespace="total.data">
 	<select id="selAll"  parameterType="TotalData" resultType="TotalData">
		SELECT * from prison_t.copa_data_aggregation;
 	</select>
 	<select id="selByCODE"  parameterType="String" resultType="TotalData">
		SELECT * from prison_t.copa_data_aggregation where zf_bh=#{zf_bh};
 	</select>
	
	<select id="updataOne" parameterType="TotalData" resultType="int">
 		update prison_t.copa_data_aggregation set method=#{method} where zf_bh=#{zf_bh}
 	</select>
</mapper>